                    <item name="CodeInput" type_name="gh_string" type_code="10">import scriptcontext as sc
import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import Rhino.DocObjects as rd
import re

class helper:
        
    ### BASIC HELPER FUNCTION
    def convertToGeometry(cls,objectList):
        res = []
        for i in range(len(objectList)):
            res.append(objectList[i].Geometry)
        return res

    def printCurveObjectLength(cls,curveObjects):
        for i in range(len(curveObjects)):
            print(curveObjects[i].Geometry.GetLength())
      
    ### DETERMINE CURVE OBJECT RELATIONSHIP  
    def doesContain(cls,curveObject1, curveObject2):
        `"""
        Determine if curveObject1 contains curveObject2:
            1. curveObject1 area is larger than curveObject2 area
            2. there has to be more than 80% overlap in between
        """`
        #corner case
        if curveObject1==curveObject2:
            return False
        
        #overlapped ration defined here
        ratio = 0.8
        
        crv1 = curveObject1.Geometry
        crv2 = curveObject2.Geometry
        
        # project curve into same plane
        point = rg.Point3d(0,0,0)
        plane = rg.Plane(point,rg.Vector3d.ZAxis)
        crv1 = rg.Curve.ProjectToPlane(crv1, plane)
        crv2 = rg.Curve.ProjectToPlane(crv2, plane)
        
        # create breps from thsoe two planar curves
        brep1=rg.Brep.CreatePlanarBreps(crv1)[0]
        area1 = brep1.GetArea()
        brep2=rg.Brep.CreatePlanarBreps(crv2)[0]
        area2 = brep2.GetArea()
        
        # if area1&lt; area2 then return False
        if area1&lt;area2:
            return False
        
        # intersec thsoe two surface
        intersect = rg.Intersect.Intersection.BrepBrep(brep1,brep2,0.001)[1]
       
        # if no intersection
        if len(intersect)==0:
            return False
        brep3=rg.Brep.CreatePlanarBreps(intersect[0])[0]
        area3 = brep3.GetArea()
   
        area_small = area1
        if area2&lt;area1:
            area_small=area2
        if area3/area_small&lt;ratio:
            return False
        else:
            return True
      
    def isChild(cls,parent,child,sortedCurveObjects):
        `"""
         only if parent contains child, and no other curveObjects contains child, return true
        """`
        if not(cls.doesContain(parent,child)):
            return False
        
        start = 0
        for i in range(len(sortedCurveObjects)):
            potentialParent = sortedCurveObjects[i]
            if potentialParent == parent:
                start = i+1
                break
        for i in range(start,len(sortedCurveObjects)):
            potentialParent = sortedCurveObjects[i]
            if(cls.doesContain(potentialParent,child)):
                return False     
        return True
        
    ### STRING PROCESSING
    def flagFinder(cls,str):
        st = re.search(r'\d+', str)
        res = re.search(r'res', str)
        retail = re.search(r'retail', str)
        office = re.search(r'office', str)
        
        if retail:
            program = retail.group()
        elif office:
            program = office.group()
        else:
            program = "res"           
            
        if not st:
            return None
        else:
             return(int(st.group()),program)           

