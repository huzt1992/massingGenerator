class BuildEach():                   
                                     
    def __init__(self, curveObject, program, base_height, st, special):
        self.curve = curveObject.Geometry
        
        # determine the height based on program
        if(program.lower() == "retail"):
            self.height = 4000
        elif(program.lower()=="office"):
            self.height = 4000
        else:
            self.height = 3000
        
        self.base_height = base_height
        self.st = st
        self.top = st*self.height+base_height
        self.buildEach = self.buildEach()
        
        # FUTURE REVISION,MIGHT USE OVERLOAD!
        self.special = special
        
    
    def buildEach(self):
        building = []
        for i in range(self.st):
            building.extend(self.buildStorey(i))
        return building
     
    def buildStorey(self, i):
        z_v = rg.Transform.Translation(rg.Vector3d(0,0,i*self.height+self.base_height))
        wall = self.buildWall()
        floor =self.buildFloor()
        wall.Transform(z_v)
        floor.Transform(z_v)
        return [wall,floor]
                                
    def buildWall(self):
        return rg.Extrusion.Create(self.curve,self.height,False)
        
    def buildFloor(self):
        return rg.Brep.CreatePlanarBreps(self.curve)[0]

class BuildBuilding():
    `"""
     input : roots as building bases(&gt;=1), graph
     output: building mass
    """`
    def __init__(self,roots,graph,match_dict):
        self.roots= roots
        self.graph = graph
        self.match_dict = match_dict
        self.building = []
        self.buildWhole()
    
    def buildWhole(self):
        for i in range(len(self.roots)):
            self.building.extend(self.buildBlock(self.roots[i]))
            
    def buildBlock(self,root):
        Block = []
        st, program = helper.flagFinder(self.match_dict[root].Geometry.Text)
        if(st == None):
            helper.promptWarning("Need storey number input!")
            return
        ##NEEDED TO BE REVISED IN FUTURE
        build = BuildEach(root,program,0,st,"base")
        Block.extend(build.buildEach)
        ##NEEDED TO BE REVISED IN FUTURE
        
        # Recursive construct
        built = set()
        built.add(root)
        self.dfsConstruct(Block,root,build.top,built)
        
        return Block
   
    def dfsConstruct(self,Block,parent,base_height,built):
       children = self.graph[parent]
       for child in children:
           if child in built:
               continue
           # build 
           built.add(child)
           st, program = helper.flagFinder(self.match_dict[child].Geometry.Text)
           if(st == None):
               helper.promptWarning("Need storey number input!")
               return
           build = BuildEach(child,program,base_height,st,None)
           Block.extend(build.buildEach)
           self.dfsConstruct(Block,child,build.top,built)

