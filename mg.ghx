                    <item name="CodeInput" type_name="gh_string" type_code="10">import scriptcontext as sc
import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import Rhino.DocObjects as rd
helper = sc.sticky["helper"] 



class BuildingGraph:
    `"""
     input : sortedCurveObjects(length increment)
     output: dictionary representing graph, root node(base curve)
    """`
    def __init__(self,sortedCurveObjects):
        self.sortedCurveObjects = sortedCurveObjects
        # reverse to decrement
        self.sortedCurveObjects.reverse()
        self.Graph = {}
        self.roots = []
        self.buildGraph()
        self.getRoots()
        
    def addNode(self,curveObject):
        if not (curveObject in self.Graph):
            self.Graph[curveObject] =[]
       
    def addChild(self,curveObject1,curveObject2):
        self.Graph[curveObject1].append(curveObject2)
     
    def buildGraph(self):
        l = len(self.sortedCurveObjects)
        for i in range(l):
            parent = self.sortedCurveObjects[i]
            if not(parent in self.Graph):
                self.addNode(parent)
            # Safely assume there is no child which has larger foot print than parent
            for j in range(i+1,l):
                child = self.sortedCurveObjects[j]
                if  helper.isChild(parent,child,self.sortedCurveObjects):
                    self.addChild(parent,child)
    
    def getRoots(self):
       `"""
       return an array of root
       """`
       seen = set()
       for crv in self.Graph:
           values = self.Graph[crv]
           for j in range(len(values)):
               seen.add(values[j])
               
       for crv in self.Graph:
           if not(crv in seen):
               self.roots.append(crv)
       
    #DEBUG ONLY
    def printGraphWithTextDict(self,match_dict):
        for crv in self.Graph:
            print(crv.Geometry.GetLength(),":")
            print("Text Notation is:",match_dict[crv].Geometry.Text)
            values = self.Graph[crv]
            for i in range(len(values)):
                print("  ",values[i].Geometry.GetLength())
            print("\n")

    #DEBUG ONLY
    def printRoots(self,match_dict):
        for crv in self.roots:
            print("Root: ",crv.Geometry.GetLength())
            print("Text Notation is:",match_dict[crv].Geometry.Text)
            print("\n")

