import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import Rhino.DocObjects as rd
helper = sc.sticky["helper"] 

# match the text to curve selected
class cuvrveTextMatch:
    
    # input : curveObjects, textObject
    # desired output : dictionary with curve-text matches -&gt; curveTextDict.curveTextDict
    def __init__(self,curveObjects,textObjects):
        self.curveObjects = curveObjects
        self.sortedCurveObjects = self.sortCurveObjects() 
        self.textObjects = textObjects
        self.textList = helper.convertToGeometry(textObjects)
        self.centers = self.getCenters()
        self.curveTextDict = self.getDict()


    def getDict(self):
        `"""`
        Return a dict:
            key : curveObject
            value : textObject
        `"""`

        # initate dict and set
        dict = {}
        seen = set()
        # match text into curve, if the text center is inside curve closure
        for i in range(len(self.sortedCurveObjects)):
            curve = self.sortedCurveObjects[i].Geometry
            # iterate all centers to find if it is inside this curve
            for j in range(len(self.centers)):
                # Create flat plane
                plane = rg.Plane(self.centers[j],rg.Vector3d.ZAxis)
                # Project the curves onto the plane
                curve = rg.Curve.ProjectToPlane(curve, plane)   
                if not(j in seen) and curve.Contains(self.centers[j],plane)== rg.PointContainment.Inside:
                    dict[self.sortedCurveObjects[i]]=self.textObjects[j]
                    seen.add(j)
                    break
        return dict
        
        
    def getCenters(self):
        centers = []
        for i in range(len(self.textList)):
            centers.append(self.textList[i].GetBoundingBox(True).Center)
        return centers
     
    def curveObjectLength(self,crv):
        return crv.Geometry.GetLength() 
    def sortCurveObjects(self):
        return sorted(self.curveObjects,key = self.curveObjectLength) 
    def printMatch(self):
        for curve in self.curveTextDict:
            print("Curve length is:",curve.Geometry.GetLength()," Text is ",self.curveTextDict[curve].Geometry.Text)


