                    <item name="CodeInput" type_name="gh_string" type_code="10">from ghpythonlib.componentbase import executingcomponent as component
import Grasshopper, GhPython
import System
import Rhino
import scriptcontext as sc
import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import Rhino.DocObjects as rd
doc = Rhino.RhinoDoc.ActiveDoc


construction = sc.sticky["construction"] 
helper = sc.sticky["helper"] 
curveTextMatch = sc.sticky["curveTextMatch"] 
BuildingGraph = sc.sticky["BuildingGraph"]
    
class MyComponent(component):
    
    def RunScript(self, x, y):


    
        class Build():                   
                                             
            def __init__(self, curveObject, height, base_height, st):
                self.curve = curveObject.Geometry
                self.height = height
                self.base_height = base_height
                self.st = st
                self.top = st*height+base_height
                self.building = self.buildWhole()
                
            
            def buildWhole(self):
                building = []
                for i in range(self.st):
                    building.extend(self.buildStorey(i))
                return building
             
            def buildStorey(self, i):
                z_v = rg.Transform.Translation(rg.Vector3d(0,0,i*self.height+self.base_height))
                wall = self.buildWall()
                floor =self.buildFloor()
                wall.Transform(z_v)
                floor.Transform(z_v)
                return [wall,floor]
                                        
            def buildWall(self):
                return rg.Extrusion.Create(self.curve,self.height,False)
                
            def buildFloor(self):
                return rg.Brep.CreatePlanarBreps(self.curve)[0]                
        class filterObjects():
            def __init__(self, list, curveList, textList):
                self.list = list
                self.curveList = curveList
                self.textList = textList
            
            def getList(self):
                for i in range(len(self.list)):
                    # isCurve
                    if self.list[i].ObjectType== Rhino.DocObjects.ObjectType.Curve:
                        self.curveList.append(self.list[i])
                    # isAnnotation
                    elif self.list[i].ObjectType == Rhino.DocObjects.ObjectType.Annotation:
                        self.textList.append(self.list[i])

                  
        # GET curveObjects, textObjects
        curveObjects = []
        textObjects = []        
        filter = filterObjects(x,curveObjects,textObjects)
        filter.getList()
        
        # GET match_dict
        CTMatch= curveTextMatch(curveObjects,textObjects)
        match_dict = CTMatch.curveTextDict
        sortedCurveObjects = CTMatch.sortedCurveObjects
        
        # BUILD graph
        G = BuildingGraph(sortedCurveObjects)
        graph = G.Graph
        roots = G.roots
        G.printRoots(match_dict)
        G.printGraphWithTextDict(match_dict)




